{
	"schema": "a2demo",
	"model": "Agent",
	"dialogs": {
		"browseCustomer": {
			"index": true,
			"model": "Customer.Browse",
			"view": "browseCustomer.dialog"
		},
		"browseSupplier": {
			"index": true,
			"model": "Supplier",
			"view": "browseSupplier.dialog"
		},
		"editCustomer": {
			"model": "Agent",
			"view": "editCustomer.dialog",
			"template": "editCustomer.template"
		},
		"editSupplier": {
			"model": "Agent",
			"view": "editSupplier.dialog",
			"template": "editSupplier.template"
		},
		"copySupplier": {
			"copy": true,
			"model": "Agent",
			"view": "editSupplier.dialog",
			"template": "editSupplier.template"
		},
		"editFolder": {
			"model": "Agent",
			"view": "editFolder.dialog",
			"template": "editFolder.template"
		}
	},
	"popups": {
		"state": {
			"model": "Agent.State",
			"view": "state.popup",
			"checkTypes": 'state.d'
		}
	},
	"commands": {
		"duplicateCode": {
			"type": "sql",
			"procedure": "Agent.Code.CheckDuplicate"
		},
		"sleep": {
			"type": "sql",
			"procedure": "Sleep"
		},
		"getNumber": {
			"type": "sql",
			"procedure": "GetNumber"
		}
	},
	"files": {
		"attachment": {
			"type": "sql",
			"model": "Agent.Attachment"
		}
	}
}
