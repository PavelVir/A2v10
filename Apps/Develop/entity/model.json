{
	"schema": "a2demo",
	"model": "Entity",
	"dialogs": {
		"browseGoods": {
			"index": true,
			"model": "Goods",
			"view": "browseGoods.dialog"
		},
		"editGoods": {
			"view": "editGoods.dialog",
			"template": "editGoods.template",
			"checkTypes": "edit.d"
		}
	},
	"commands": {
		"findArticle": {
			"type": "sql",
			"procedure": "Entity.FindArticle"
		},
		"duplicateArticle": {
			"type": "sql",
			"procedure": "Entity.Article.CheckDuplicate"
		}
	},
	"files": {
		"myfile": {
			"type": "azureBlob",
			"container": "images",
			"model": "Entity.Image"
		}
	}
}
